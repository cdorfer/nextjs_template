# Direct connection to the database. Used for migrations.
DIRECT_URL="postgres://postgres:<Project PW>@db.<Reference ID>.supabase.co:5432/postgres"

NEXTAUTH_SECRET=someSecretHere
NEXTAUTH_URL=http://localhost:3000